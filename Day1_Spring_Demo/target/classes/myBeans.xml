<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<bean id="emp" class="org.cap.model.Employee" autowire="byName" init-method="start" destroy-method="destroy" lazy-init="true">


	<constructor-arg name="employeeId" value="1001"></constructor-arg>
	<constructor-arg name="employeeName" value="Poonam"></constructor-arg>
	<constructor-arg name="employeeSalary" value="50000"></constructor-arg>

	<!-- <property name="employeeId" value="1001"></property>
	<property name="employeeName" value="Poonam"></property>
	<property name="employeeSalary" value="50000"></property> -->


</bean>


</beans>
